# TextIn API credentials (required)
TEXTIN_APP_ID=your_app_id
TEXTIN_SECRET_CODE=your_secret_code

# PostgreSQL connection string
DATABASE_URL=postgresql+psycopg://user:password@localhost:5432/doc_parser

# Local storage root (parsed outputs only â€” raw files are not stored)
DATA_DIR=data

# TextIn parse settings
TEXTIN_PARSE_MODE=auto
TEXTIN_MAX_CONCURRENT=3

# Google Drive auth
GOOGLE_CREDENTIALS_FILE=credentials.json
GOOGLE_SERVICE_ACCOUNT=false

# Extraction provider: "textin" (default) or "llm"
# EXTRACTION_PROVIDER=textin

# LLM extraction settings (OpenRouter / OpenAI-compatible)
# LLM_API_KEY=your_openrouter_api_key
# LLM_BASE_URL=https://openrouter.ai/api/v1
# LLM_MODEL=openai/gpt-4o-mini
# LLM_MAX_TOKENS=1024
# LLM_TEMPERATURE=0.0
# LLM_CONTEXT_CHARS=4000
